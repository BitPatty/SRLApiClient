<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp2.0</TargetFramework>
    <AssemblyVersion>0.3.0.0</AssemblyVersion>
    <FileVersion>0.3.0.0</FileVersion>
    <Version>0.3.0</Version>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>BitPatty &lt;psy@zint.ch&gt;</Authors>
    <Description>.NET Core API Wrapper for the SpeedRunsLive API</Description>
    <RepositoryUrl>https://github.com/BitPatty/SRLApiClient</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageProjectUrl>https://github.com/BitPatty/SRLApiClient</PackageProjectUrl>
    <PackageLicenseUrl>https://www.gnu.org/licenses/agpl-3.0.en.html</PackageLicenseUrl>
    <PackageReleaseNotes>.NET Core API Wrapper for the SpeedRunsLive API</PackageReleaseNotes>
    <PackageTags>speedrunslive</PackageTags>
    <Copyright>2018 Matt Collet</Copyright>
  </PropertyGroup>

  <ItemGroup>
    <Reference Include="System">
      <HintPath>System</HintPath>
    </Reference>
  </ItemGroup>

</Project>

{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "SRLApiClient.csproj"
          ],
          "src": "."
        }
      ],
      "dest": "docfx/api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/**.md",
        ],
        "src": "docfx",
        "dest": "api"
      },
      {
        "files": [
          "articles/**.md",
          "articles/**.yml"
        ],
        "src": "docfx",
        "dest": "articles"
      },
      {
        "files": [
          "**.yml",
          "**.md"
        ],
        "src": "docfx",
        "dest": "."
      }
    ],
    "resource": [
      {
        "files": [
          "docfx/images/**"
        ]
      }
    ],
    "dest": "../docs",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}